<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
	
  	<display-name>Codinplay</display-name>
  	<welcome-file-list>
	    <welcome-file>paginas/index.html</welcome-file>
	    <welcome-file>paginas/index.htm</welcome-file>
	    <welcome-file>paginas/index.jsp</welcome-file>
	    <welcome-file>paginas/default.html</welcome-file>
	    <welcome-file>paginas/default.htm</welcome-file>
	    <welcome-file>paginas/default.jsp</welcome-file>
	    <welcome-file>paginas/index.jsf</welcome-file>
	    <welcome-file>paginas/default.jsf</welcome-file>
  	</welcome-file-list>
  	<servlet>
    	<servlet-name>Faces Servlet</servlet-name>
    	<servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
    	<load-on-startup>1</load-on-startup>
  	</servlet>
  	<servlet-mapping>
    	<servlet-name>Faces Servlet</servlet-name>
    	<url-pattern>*.jsf</url-pattern>
  	</servlet-mapping>
  	<context-param>
		<param-name>javax.faces.PROJECT_STAGE</param-name>
		<param-value>Development</param-value>
  	</context-param>
 	 <context-param>
    	<param-name>javax.servlet.jsp.jstl.fmt.localizationContext</param-name>
   		<param-value>resources.application</param-value>
  	</context-param>

 	<listener>
    	<listener-class>com.sun.faces.config.ConfigureListener</listener-class>
  	</listener>
	

</web-app>